{
  "env": {
    "commonjs": true,
    "es2021": true,
    "browser": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "operator-linebreak": "off",
    "no-trailing-spaces":"off",
    "eol-last":"off",
    "no-plusplus":"off"
  }
}
